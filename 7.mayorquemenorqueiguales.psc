Proceso mayoromenor
	Definir N1 Como Entero;
	Definir N2 Como Entero;
	N1<-0;
	N2<-0;
	Escribir "digita N1";
	Leer N1;
	Escribir "digita N2";
	Leer N2;
	si N1>N2 Entonces
		Escribir "N1 Mayor N2";
	Sino
	FinSi
	Si N1=N2 Entonces
		Escribir " Iguales";
	Sino
	si N1<N2 Entonces
	    Escribir " N1 Menor N2";
	Sino
		FinSi
	FinSi
FinProceso

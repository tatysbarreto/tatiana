Proceso igualdiferente
	Definir N1 Como Entero;
	Definir N2 Como Entero;
	Definir Ac Como Entero;
	Definir Ar Como Entero;
	N1<-0;
	N2<-0;
	Escribir "digita N1";
	Leer N1;
	Escribir "digita N2";
	Leer N2;
	si N1=N2 Entonces
		Ac<-((N1)*(N2));
		Escribir "area del cuadrado"," ",Ac;
	Sino
		Ar<-((N1)*(N2));
		Escribir "el area del rectangulo"," ",Ar;
	FinSi
FinProceso
